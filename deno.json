{
  "name": "@frontside/revolution",
  "license": "ISC",
  "publish": {
    "exclude": [
      "test"
    ]
  },
  "tasks": {
    "test": "deno test --allow-read --allow-write --allow-env --allow-run --allow-net",
    "build:npm": "deno run -A tasks/build-npm.ts"
  },
  "exports": {
    ".": "./mod.ts",
    "./jsx-runtime": "./jsx-runtime.ts"
  },
  "lint": {
    "rules": {
      "exclude": ["prefer-const", "require-yield", "no-slow-types"]
    }
  },
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "revolution"
  },
  "imports": {
    "deno-dom": "jsr:@b-fuze/deno-dom@0.1.48",
    "deno-dom/wasm": "https://deno.land/x/deno_dom@v0.1.41/deno-dom-wasm.ts",
    "deno-dom/constructor-lock": "https://deno.land/x/deno_dom@v0.1.41/src/constructor-lock.ts",
    "esbuild": "npm:esbuild@0.24.2",
    "esbuild-deno-loader": "jsr:@luca/esbuild-deno-loader@0.11.1",
    "effection": "jsr:@effection/effection@3.6.1",
    "@std/assert": "jsr:@std/assert@1.0.10",
    "@std/path": "jsr:@std/path@1.0.8",
    "@std/http": "jsr:@std/http@1.0.12",
    "hast": "npm:@types/hast@^3.0.0",
    "hast-util-to-html": "npm:hast-util-to-html@9.0.0",
    "hastx": "npm:hastx@0.0.11",
    "@std/testing": "jsr:@std/testing@1.0.8",
    "revolution/jsx-runtime": "./jsx-runtime.ts",
    "path-to-regexp": "npm:path-to-regexp@8.2.0",
    "@deno/dnt": "jsr:@deno/dnt@0.41.3",
    "expect": "https://deno.land/x/expect@v0.2.9/mod.ts"
  }
}
